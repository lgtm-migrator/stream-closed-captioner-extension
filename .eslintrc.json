{
    "env": {
      "browser": true
    },
    "extends": ["zeal", "zeal/ramda", "zeal/react", "prettier", "prettier/react", "zeal/jest"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 7,
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "plugins": [
    ],
    "rules": {
        "no-process-env": "off",
        "semi": [
            "error",
            "never"
        ]
    },
    "settings": {
      "react": {
        "version": "detect"
      },
      "import/resolver": "webpack"
    }
}
